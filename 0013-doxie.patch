diff --git a/common.mk b/common.mk
index dcdaa6f..a912b2f 100644
--- a/common.mk
+++ b/common.mk
@@ -55,16 +55,13 @@ ifndef VERSION
 ###  DATE_EN           := $(shell LC_ALL=C date --date="$(DATE)" '+%d %B, %Y')
   export VERSION DATE DATE_EN
 
-  ifndef DISTIBUTOR
+  ifndef DISTRIBUTOR
     ifneq (,$(findstring ubuntu,$(VERSION)))
       DISTRIBUTOR     := Ubuntu
     else
       DISTRIBUTOR     := `lsb_release -is`
     endif
   endif
-  ifneq (Ubuntu,$(DISTRIBUTOR))
-    DISTRIBUTOR   := Debian
-  endif
   distributor     := $(shell echo $(DISTRIBUTOR) | tr "[:upper:]" "[:lower:]")
 
   export DISTRIBUTOR distributor
diff --git a/data/dwww.css.in b/data/dwww.css.in
index e9408a5..661884a 100644
--- a/data/dwww.css.in
+++ b/data/dwww.css.in
@@ -10,7 +10,7 @@
 */
 
